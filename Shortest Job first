// Online C++ compiler to run C++ program online
#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int>bt={4,3,7,1,2};
    sort(bt.begin(),bt.end());
    int t=0,ans=0;
    for(int i=0;i<bt.size();i++)
    {
       
           ans+=t;
           t+=bt[i];
        
    }
    cout<<(ans/bt.size())<<endl;
    return 0;
}
